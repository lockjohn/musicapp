<form action="<%=albums_url%>" method="post">
  <%= auth_token %>
  <select name="band">
    <% Bands.all.each do |band| %>
    <option value=" <%= band.name %>"><%=band.name%></option>
    <% end %>
  </select>
  live<input type="radio" name="Recorded" value="live">
  studio<input type="radio" name="Recorded" value="studio" checked="true">
  <input type="submit" class="button" value="Add Album">
</form>
